{
    "manifest_version": 2,
    "name": "Auto-bookmark",
    "version": "0.1",

  "permissions": [
    "bookmarks",
    "tabs"
  ],
  
    "browser_action": {
      "default_icon": {
        "48": "icons/auto-bookmark-v2.png"
      },
      "default_title": "Auto-bookmark",
      "default_popup": "/popup.html"
    }

}